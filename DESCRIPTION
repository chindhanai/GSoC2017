    R (>= 3.0.0),
    xts (>= 0.9),
    foreach (>= 1.4),
    rrcov (>= 1.3)
Imports:
    PerformanceAnalytics(>= 1.4),
    quantmod,
    zoo,
    corrplot,
    robustbase,
    robust,
    lattice,
    MASS,
    DEoptimR,
    mvtnorm
LazyData: true
